{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0.1",
  


    "action":{
        "default_popup": "/views/login.html",
        "default_title": "Latest Covid Report",

        "default_icon": {
            "128": "/assets/icons/128.png"
        }
    },
    "default_locale": "en",
    "description": "A plain text description",
    "icons": {
        "128": "/assets/icons/128.png"
    },
  


    "author": "Jingheng Pan, Adrian Lindloff, Lijunnan Bai",

    "background": {
        "service_worker": "background.js"
    },
    "options_page": "",
    "content_scripts": [
    ],

    "host_permissions": [
        "http://*/*", 
        "https://*/*",
        "https://developer.chrome.com/*"
    ],
    "permissions": [
        "system.cpu",
        "system.memory",
        "system.storage",
        "system.display",
        "contextMenus",

        "storage",
        "activeTab",
        "tabs",
        "scripting",
        "history",
        "notifications",
        "cookies"
    ],
    "requirements": {

    },

    "commands": {
        "_execute_action": {
          "suggested_key": {
            "default": "Ctrl+U",
            "mac": "Command+U"
          }
        }
    }
}